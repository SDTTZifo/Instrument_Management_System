// src/App.js
import React from 'react';
import { BrowserRouter as Router, Route, Switch } from 'react-router-dom';
import AdminDashboard from './components/Admins/AdminDashboard';
import CoordinatorDashboard from './components/Coordinators/CoordinatorDashboard';
import TrainerDashboard from './components/Trainers/TrainerDashboard';
import TraineeDashboard from './components/Trainees/TraineeDashboard';

function App() {
  return (
    <Router>
      <div className="App">
        <Switch>
          <Route path="/admin" component={AdminDashboard} />
          <Route path="/coordinator" component={CoordinatorDashboard} />
          <Route path="/trainer" component={TrainerDashboard} />
          <Route path="/trainee" component={TraineeDashboard} />
          <Route path="/" exact component={() => <div>Home Page</div>} />
        </Switch>
      </div>
    </Router>
  );
}

export default App;
